<table class="table table-striped">
  <thead>
    <tr>
      <th>Dojo</th>
      <th>Dates</th>
    </tr>
  </thead>
  <tbody>
    <% @dojo_history.each do |ds| %>
      <tr>
        <td><%= link_to ds.dojo.name, dojo_path(ds.dojo.id) %></td>
        <td>
          <%= ds.start_date.strftime("%m/%d/%y") + " - " + (ds.end_date.nil? ? "Today" : ds.end_date.strftime("%m/%d/%y")) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @dojo_history, :previous_label => "Previous", :next_label => "Next", param_name: "dojo_page" %>
